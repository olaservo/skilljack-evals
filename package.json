{
  "name": "@skilljack/evals",
  "version": "1.0.0",
  "type": "module",
  "description": "CLI for evaluating AI agent skill discoverability, adherence, and output quality. Runs as standalone CLI or GitHub Action.",
  "author": "Ola Hungerford",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/olaservo/skilljack-evals.git"
  },
  "homepage": "https://github.com/olaservo/skilljack-evals",
  "keywords": [
    "agent-skills",
    "evaluation",
    "llm",
    "testing",
    "skill-discovery",
    "claude",
    "github-action"
  ],
  "bin": {
    "skilljack-evals": "./dist/src/cli.js"
  },
  "main": "./dist/src/index.js",
  "types": "./dist/src/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/src/index.js",
      "types": "./dist/src/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsx src/cli.ts",
    "start": "node dist/src/cli.js",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@anthropic-ai/claude-agent-sdk": "~0.1.76",
    "commander": "^12.0.0",
    "dotenv": "^16.4.0",
    "js-yaml": "^4.1.0"
  },
  "devDependencies": {
    "@actions/core": "^1.10.0",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^20.0.0",
    "tsx": "^4.0.0",
    "typescript": "^5.0.0"
  },
  "peerDependencies": {
    "@ai-sdk/anthropic": "^3.0.0",
    "@ai-sdk/google": "^3.0.0",
    "@ai-sdk/openai": "^3.0.33",
    "@openai/agents": "^0.4.15",
    "ai": "^6.0.99",
    "openai": "^6.25.0",
    "zod": "^4.0.0"
  },
  "peerDependenciesMeta": {
    "ai": {
      "optional": true
    },
    "@ai-sdk/openai": {
      "optional": true
    },
    "@ai-sdk/anthropic": {
      "optional": true
    },
    "@ai-sdk/google": {
      "optional": true
    },
    "@openai/agents": {
      "optional": true
    },
    "openai": {
      "optional": true
    },
    "zod": {
      "optional": true
    }
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "files": [
    "dist",
    "action"
  ]
}
